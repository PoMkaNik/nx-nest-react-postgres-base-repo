{
    "name": "eslint-plugin",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/eslint-plugin/src",
    "projectType": "library",
    "targets": {
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/eslint-plugin/**/*.js"],
                "fix": true
            }
        }
    },
    "tags": ["type:lib", "eslint"]
}
